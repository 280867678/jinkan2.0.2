<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android" android:orientation="vertical" android:layout_width="match_parent" android:layout_height="match_parent">
    <LinearLayout android:orientation="vertical" android:background="@drawable/shape_bg_dialog_parse" android:paddingLeft="10dp" android:paddingTop="18dp" android:paddingRight="10dp" android:descendantFocusability="blocksDescendants" android:layout_width="match_parent" android:layout_height="wrap_content">
        <LinearLayout android:orientation="horizontal" android:layout_width="match_parent" android:layout_height="75dp">
            <LinearLayout android:gravity="center" android:orientation="vertical" android:id="@+id/llyt_chane_web" android:layout_width="0dp" android:layout_height="match_parent" android:layout_weight="1">
                <ImageView android:layout_width="26dp" android:layout_height="26dp" android:src="@drawable/ic_web_nav_change_webview"/>
                <TextView android:textSize="11sp" android:textColor="@color/text_black" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_marginTop="7dp" android:text="UA切换"/>
            </LinearLayout>
            <LinearLayout android:gravity="center" android:orientation="vertical" android:id="@+id/llyt_chane_player" android:layout_width="0dp" android:layout_height="match_parent" android:layout_weight="1">
                <ImageView android:layout_width="26dp" android:layout_height="26dp" android:src="@drawable/ic_web_nav_change_player"/>
                <TextView android:textSize="11sp" android:textColor="@color/text_black" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_marginTop="7dp" android:text="播放模式"/>
            </LinearLayout>
            <LinearLayout android:gravity="center" android:orientation="vertical" android:id="@+id/llyt_download" android:layout_width="0dp" android:layout_height="match_parent" android:layout_weight="1">
                <ImageView android:id="@+id/iv_no_pic" android:layout_width="26dp" android:layout_height="26dp" android:src="@drawable/ic_menu_download_gray2"/>
                <TextView android:textSize="11sp" android:textColor="@color/text_black" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_marginTop="7dp" android:text="下载管理"/>
            </LinearLayout>
            <LinearLayout android:gravity="center" android:orientation="vertical" android:id="@+id/llyt_adb" android:layout_width="0dp" android:layout_height="match_parent" android:layout_weight="1">
                <ImageView android:id="@+id/iv_adb" android:layout_width="26dp" android:layout_height="26dp" android:src="@drawable/ic_web_nav_adb_open"/>
                <TextView android:textSize="11sp" android:textColor="@color/text_black" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_marginTop="7dp" android:text="广告过滤"/>
            </LinearLayout>
        </LinearLayout>
        <LinearLayout android:orientation="horizontal" android:layout_width="match_parent" android:layout_height="75dp">
            <LinearLayout android:gravity="center" android:orientation="vertical" android:id="@+id/llyt_go_setting" android:layout_width="0dp" android:layout_height="match_parent" android:layout_weight="1">
                <ImageView android:layout_width="26dp" android:layout_height="26dp" android:src="@drawable/ic_web_nav_setting"/>
                <TextView android:textSize="11sp" android:textColor="@color/text_black" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_marginTop="7dp" android:text="系统设置"/>
            </LinearLayout>
            <LinearLayout android:gravity="center" android:orientation="vertical" android:id="@+id/llyt_history" android:layout_width="0dp" android:layout_height="match_parent" android:layout_weight="1">
                <ImageView android:layout_width="wrap_content" android:layout_height="26dp" android:src="@drawable/ic_web_nav_history"/>
                <TextView android:textSize="11sp" android:textColor="@color/text_black" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_marginTop="7dp" android:text="播放历史"/>
            </LinearLayout>
            <LinearLayout android:gravity="center" android:orientation="vertical" android:id="@+id/llyt_share" android:layout_width="0dp" android:layout_height="match_parent" android:layout_weight="1">
                <ImageView android:layout_width="wrap_content" android:layout_height="26dp" android:src="@drawable/ic_web_nav_share"/>
                <TextView android:textSize="11sp" android:textColor="@color/text_black" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_marginTop="7dp" android:text="分享网址"/>
            </LinearLayout>
            <LinearLayout android:gravity="center" android:orientation="vertical" android:id="@+id/llyt_go_exit" android:layout_width="0dp" android:layout_height="match_parent" android:layout_weight="1">
                <ImageView android:layout_width="26dp" android:layout_height="26dp" android:src="@drawable/ic_web_nav_close"/>
                <TextView android:textSize="11sp" android:textColor="@color/text_black" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_marginTop="7dp" android:text="退出浏览"/>
            </LinearLayout>
        </LinearLayout>
        <View android:background="@color/line_color" android:layout_width="match_parent" android:layout_height="0.5dp" android:layout_marginLeft="20dp" android:layout_marginTop="18dp" android:layout_marginRight="20dp"/>
        <LinearLayout android:orientation="horizontal" android:layout_width="match_parent" android:layout_height="45dp">
            <LinearLayout android:gravity="center" android:id="@+id/llyt_dismiss" android:layout_width="0dp" android:layout_height="match_parent" android:layout_weight="1">
                <ImageView android:layout_width="25dp" android:layout_height="25dp" android:src="@drawable/ic_web_nav_dismiss"/>
            </LinearLayout>
        </LinearLayout>
    </LinearLayout>
</LinearLayout>
